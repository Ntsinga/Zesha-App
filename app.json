{
  "expo": {
    "name": "Zesha Agent Banking",
    "slug": "z",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/background.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zesha.agentbanking"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zesha.agentbanking"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "scheme": "zesha",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fe756f9a-6158-4fba-ba42-a0573c86f372"
      }
    },
    "owner": "zesha-capital-enterprises"
  }
}
